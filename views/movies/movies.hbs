<div class="movies-list">

    <h2>All Movies</h2>


    <div class="movies">
        <div class="row">

            {{#each movies}}

            <div class="col-md-4">
                <div class="card mb-4 artist-card">

                    <div class="card-name">

                        <h3>{{title}}</h3>
                        <a href="/movie-details/{{_id}}">Info</a>
                    </div>

                </div>
            </div>




            {{/each}}



        </div>

    </div>



</div>